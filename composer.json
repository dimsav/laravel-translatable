{
    "name": "dimsav/laravel-translatable",
    "description": "A Laravel package for multilingual models",
    "keywords": ["laravel", "translation", "language", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitrios Savvopoulos",
            "email": "ds@dimsav.com",
            "homepage": "http://dimsav.com",
            "role": "Developer"
        },
        {
            "name": "Tom Witkowski",
            "email": "dev.gummibeer@gmail.com",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dimsav/laravel-translatable/issues"
    },
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*|3.7.*|3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Dimsav\\Translatable\\": "src/Translatable/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dimsav\\Translatable\\TranslatableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
